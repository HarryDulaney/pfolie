{
  "name": "pfolie",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start:local:all": "start npm run start:dev:backend && start npm run start:dev:frontend",
    "start": "node api/server.js",
    "start:dev:frontend": "ng serve --open",
    "start:dev:backend": "nodemon --watch dist --watch api api/server.js",
    "start:local": "npm run build --watch && npm run start",
    "build": "ng build",
    "build:dev": "ng build --aot --watch=true --output-hashing=none",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "heroku-prebuild": "npm install --prefix api && npm install",
    "heroku-postbuild": "ng build --configuration production"
  },
  "author": "Harry Dulaney",
  "license": "GNUv3",
  "private": true,
